# Primary AI (OpenAI-compatible)
AI_API_KEY=your-primary-ai-key
AI_BASE_URL=https://cdn.12ai.org/v1
AI_MODEL=claude-sonnet-4-5-20250929

# Fallback AI (optional)
AI_FALLBACK_API_KEY=your-fallback-ai-key
AI_FALLBACK_BASE_URL=https://api.deepseek.com/v1
AI_FALLBACK_MODEL=deepseek-chat

# OCR (vision) dedicated config (recommended)
# Many chat-only models (e.g. deepseek-chat) do NOT support image_url vision inputs.
# If you use screenshots OCR, set AI_OCR_* to a vision-capable model/provider.
AI_OCR_API_KEY=
AI_OCR_BASE_URL=
AI_OCR_MODEL=
AI_OCR_FALLBACK_API_KEY=
AI_OCR_FALLBACK_BASE_URL=
AI_OCR_FALLBACK_MODEL=

# Growth mode (optional)
NEXT_PUBLIC_GROWTH_MODE=true
GROWTH_MODE=true

# Paywall (optional)
PAYWALL_SECRET=change_me_to_a_long_random_string
UNLOCK_CODE_SOUL=change_me
UNLOCK_CODE_FORTUNE_TAROT=change_me
UNLOCK_CODE_FORTUNE_DAILY=change_me
UNLOCK_CODE_LAUNDERER=change_me

# PostHog (optional)
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

